        <rdfs:label xml:lang="en">cliticness</rdfs:label>
        <rdfs:label xml:lang="ru">клитичность</rdfs:label>        
        <dc:creator>Francopoulo, Gil</dc:creator>
        <rdfs:comment xml:lang="en">Categorization of the different types of clitics</rdfs:comment>
        <rdfs:comment xml:lang="ru">Классификация различных типов клитик</rdfs:comment>
        <owl:versionInfo>1:0</owl:versionInfo>
        <dcr:datcat rdf:resource="http://www.isocat.org/datcat/DC-1903"/>
        <rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
        <rdfs:range rdf:resource="&lexinfo;Cliticness"/>
        <rdfs:subPropertyOf rdf:resource="&lexinfo;morphosyntacticProperty"/>
