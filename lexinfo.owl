        <rdfs:label xml:lang="ru">аппроксимация</rdfs:label>        
        <rdfs:comment xml:lang="en">Property used to qualify something similar but not exactly the same</rdfs:comment>
        <rdfs:comment xml:lang="ru">Свойство, используемое, чтобы квалифицировать нечто подобное, но не точно такое же.</rdfs:comment>        
        <dc:creator>Francopoulo, Gil</dc:creator>
        <owl:versionInfo>1:0</owl:versionInfo>
        <rdfs:subPropertyOf rdf:resource="&lemon;senseRelation"/>
        <dcr:datcat rdf:resource="http://www.isocat.org/datcat/DC-2206"/>
        <rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
    </owl:ObjectProperty>
    


