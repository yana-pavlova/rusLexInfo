        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdfs:label xml:lang="en">abbreviated form</rdfs:label>
        <rdfs:label xml:lang="ru">сокращённая форма</rdfs:label>
        <rdfs:comment xml:lang="en">A term or lexeme resulting from the omission of any part of the full term or lexeme while designating the same concept.</rdfs:comment>
        <rdfs:comment xml:lang="ru">Термин или лексема, возникающая в результате опущения какой-либо части полного термина или лексемы при обозначении того же понятия</rdfs:comment>        
        <dc:creator>Wright, Sue Ellen</dc:creator>
        <owl:versionInfo>1:0</owl:versionInfo>
        <dcr:datcat rdf:resource="http://www.isocat.org/datcat/DC-329"/>
        <rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
    </owl:Thing>
    


