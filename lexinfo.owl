    <!-- http://www.lexinfo.net/ontology/2.0/lexinfo#normativeAuthorization -->

    <owl:ObjectProperty rdf:about="&lexinfo;normativeAuthorization">
        <rdfs:label xml:lang="en">normative authorization</rdfs:label>
        <rdfs:label xml:lang="ru">нормативное</rdfs:label>        
        <dc:creator>Wright, Sue Ellen</dc:creator>
