    <owl:ObjectProperty rdf:about="&lexinfo;clippedTermFor">
        <rdfs:label xml:lang="en">clippedTermFor</rdfs:label>
        <rdfs:label xml:lang="ru">усеченный термин для</rdfs:label>        
        <owl:versionInfo>1:0</owl:versionInfo>
        <rdfs:comment xml:lang="en">A linking element used to identify a relation between a clipped term and its full or expanded form.</rdfs:comment>
        <rdfs:comment xml:lang="ru">Связующий элемент, используемый для идентификации отношения между сокращённым термином и его полной или расширенной формой.</rdfs:comment>
        <dc:creator>Wright, Sue Ellen</dc:creator>
        <dcr:datcat rdf:resource="http://www.isocat.org/datcat/DC-130"/>
        <rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
        <rdfs:subPropertyOf rdf:resource="&lexinfo;derivedForm"/>
    </owl:ObjectProperty>
    


