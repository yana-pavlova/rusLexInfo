    <!-- http://www.lexinfo.net/ontology/2.0/lexinfo#animacy -->

    <owl:ObjectProperty rdf:about="&lexinfo;animacy">
        <rdfs:label xml:lang="en">animacy</rdfs:label>
        <rdfs:label xml:lang="ru">одушевленность</rdfs:label>
        <owl:versionInfo>1:0</owl:versionInfo>
        <dc:creator>Francopoulo, Gil</dc:creator>
        <dc:creator>Wright, Sue Ellen</dc:creator>
        <rdfs:comment xml:lang="en">The characteristic of a word indicating that in a given discourse community, its referent is considered to be alive or to possess a quality of volition or consciousness.</rdfs:comment>
        <rdfs:comment xml:lang="ru">Характеристика слова, указывающего, что в данном дискурсе его референт считается живым или обладает качеством воли или сознания.</rdfs:comment>        
        <dcr:datcat rdf:resource="http://www.isocat.org/datcat/DC-1902"/>
        <dcr:datcat rdf:resource="http://www.isocat.org/datcat/DC-78"/>
        <rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
        <rdfs:range rdf:resource="&lexinfo;Animacy"/>
        <rdfs:subPropertyOf rdf:resource="&lexinfo;morphosyntacticProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.lexinfo.net/ontology/2.0/lexinfo#antonym -->

    <owl:ObjectProperty rdf:about="&lexinfo;antonym">
        <rdfs:label xml:lang="en">antonym term</rdfs:label>
        <rdfs:label xml:lang="ru">антонимичное понятие</rdfs:label>        
        <dc:creator>Wright, Sue Ellen</dc:creator>
        <owl:versionInfo>1:0</owl:versionInfo>
        <rdfs:comment xml:lang="en">A term or lexeme whose concept or sense constitutes the opposite of the concept represented by a second term or lexeme.</rdfs:comment>
        <rdfs:comment xml:lang="ru">Термин или лексема, понятие или смысл которых противоположны понятию, представленному вторым термином или лексемой.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&lemon;incompatible"/>
        <dcr:datcat rdf:resource="http://www.isocat.org/datcat/DC-83"/>
        <rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
    </owl:ObjectProperty>
    


    <!-- http://www.lexinfo.net/ontology/2.0/lexinfo#approximate -->

