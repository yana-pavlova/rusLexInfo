    </owl:ObjectProperty>
    


    <!-- http://www.lexinfo.net/ontology/2.0/lexinfo#aspect -->

    <owl:ObjectProperty rdf:about="&lexinfo;aspect">
        <rdfs:label xml:lang="en">aspect</rdfs:label>
        <rdfs:label xml:lang="ru">вид</rdfs:label>        
        <dc:creator>Francopoulo, Gil</dc:creator>
        <owl:versionInfo>1:0</owl:versionInfo>
        <rdfs:comment xml:lang="en">Category associated to verbs and referring to the way the grammar marks the duration or type of temporal activity.</rdfs:comment>
        <rdfs:comment xml:lang="ru">Категория, связанная с глаголом и указывающая на то, как грамматика ообозначает продолжительность или тип временного действия.</rdfs:comment>
        <dcr:datcat rdf:resource="http://www.isocat.org/datcat/DC-1242"/>
        <rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
        <rdfs:range rdf:resource="&lexinfo;Aspect"/>
        <rdfs:subPropertyOf rdf:resource="&lexinfo;morphosyntacticProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.lexinfo.net/ontology/2.0/lexinfo#associativeRelation -->

    <owl:ObjectProperty rdf:about="&lexinfo;associativeRelation">
        <rdfs:label xml:lang="en">concept relation</rdfs:label>
        <rdfs:label xml:lang="ru">отношение понятий</rdfs:label>        
        <rdfs:comment xml:lang="en">A relation between two concepts having a non-hierarchical thematic connection by virtue of experience.</rdfs:comment>
        <rdfs:comment xml:lang="ru">Отношение между двумя понятиями, имеющими неиерархическую тематическую связь на основании опыта</rdfs:comment>
        <owl:versionInfo>1:0</owl:versionInfo>
        <dc:creator>Wright, Sue Ellen</dc:creator>
        <dcr:datcat rdf:resource="http://www.isocat.org/datcat/DC-88"/>
        <rdfs:isDefinedBy rdf:resource="http://www.lexinfo.net/ontology/2.0/lexinfo"/>
        <rdfs:subPropertyOf rdf:resource="&lexinfo;relatedTerm"/>
    </owl:ObjectProperty>
    


